<%- include('./partials/loginhead.ejs') %>

<h1 class="page-heading">Register</h1>
<p>Already have an account? <a href="/login">Login here</a></p>


<div class="form-div">
  

  <form method="post" action="/register">

    <label class="label-class" for="username">Username</label>
    <input class="input-class" type="text" name="username" id="username" required />

    <label class="label-class" for="password">Password</label>
    <input class="input-class" type="password" name="password" id="password" required />

    <input class="label-class" type="submit" value="Register" id="login-button" />
  </form>
</div>
